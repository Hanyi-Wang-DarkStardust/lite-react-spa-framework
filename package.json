{
  "name": "lite-react-spa-framework",
  "private": true,
  "version": "0.0.1",
  "description": "A light weight service and model framework for SPA-oriented app",
  "main": "index.js",
  "scripts": {
    "build": "nx run-many --target=build --all --parallel=5",
    "clean": "nx run-many --target=clean --all --parallel=5",
    "clean-all": "nx run-many --target=clean-all --all --parallel=5",
    "ts-check": "tsc --noEmit --skipLibCheck",
    "lint": "eslint '@(packages)/**/*.@(ts|tsx)'",
    "lint-fix": "eslint '@(packages)/**/*.@(ts|tsx)' --fix",
    "test": "cross-env NODE_ENV=test jest --verbose",
    "postinstall": "husky install",
    "preinstall": "npx only-allow pnpm",
    "prepare": "npx husky install"
  },
  "keywords": [
    "SPA",
    "React"
  ],
  "author": "Hanyi Wang",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.17.12",
    "@babel/preset-env": "^7.14.7",
    "@changesets/cli": "^2.25.2",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nrwl/cli": "^13.10.0",
    "@nrwl/devkit": "^13.10.0",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/workspace": "^13.10.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/history": "*",
    "@types/jest": "^26.0.20",
    "@types/qs": "^6.9.7",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "^1.3.2",
    "commitlint": "^17.2.0",
    "conventional-changelog-cli": "^2.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.5.0",
    "eslint-plugin-unicorn": "^28.0.2",
    "history": "^5.3.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.3",
    "nx": "15.0.7",
    "only-allow": "^1.1.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rollup": "^2.53.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^1.2.0",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "ts-jest": "^26.4.4",
    "tsc-alias": "^1.2.8",
    "tslib": "^2.4.0",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint"
  }
}
